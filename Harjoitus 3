using System;

namespace Harjoitus_D3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Kaikki 4 peruslaskutoimitusta luvulla 8,5:");
            Console.WriteLine("8,5 + 4,5 = {0}", 8.5 + 4.5);
            Console.WriteLine("8,5 - 5,5 = {0}", 8.5 - 5.5);
            Console.WriteLine("8,5 * 5 = {0}", 8.5 * 5);
            Console.WriteLine("8,5 / 5 = {0}", 8.5 / 5);
        }
    }
}

using System;

namespace Harjoitus_D8
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Syötä kaksi lukua, niin lasken niillä kaikki 4 peruslaskutoimitusta.");
            double luku1 = double.Parse(Console.ReadLine());
            double luku2 = double.Parse(Console.ReadLine());
            Console.WriteLine("{0} {1} {2} {3}", luku1 + luku2, luku1 - luku2, luku1 * luku2, luku1 / luku2);
        }
    }
}
